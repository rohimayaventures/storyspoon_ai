{
  "name": "storyspoon-ai",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "server",
    "web"
  ],
  "scripts": {
    "dev:server": "pnpm --filter storyspoon-server dev",
    "dev:web": "pnpm --filter storyspoon-web dev",
    "dev": "concurrently -n server,web -c green,cyan \"pnpm dev:server\" \"pnpm dev:web\""
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}